<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.FXMLGameController">
   <children>
      <Canvas fx:id="gameCanvas" height="550.0" layoutX="25.0" layoutY="25.0" width="550.0" />
      <TitledPane layoutX="14.0" layoutY="14.0" minHeight="-Infinity" minWidth="-Infinity" opacity="0.9" prefHeight="201.0" prefWidth="162.0" text="Chat">
         <content>
            <Pane prefHeight="175.0" prefWidth="160.0">
               <children>
                  <Button fx:id="buttonChat" layoutX="112.0" layoutY="143.0" mnemonicParsing="false" onAction="#SetTekst" prefHeight="36.0" prefWidth="54.0" text="Send" />
                  <ListView fx:id="listChat" prefHeight="148.0" prefWidth="160.0" />
                  <TextField fx:id="textChat" layoutY="148.0" onAction="#SetTekst" prefHeight="26.0" prefWidth="117.0" />
               </children>
            </Pane>
         </content>
      </TitledPane>
      <TitledPane animated="false" layoutX="424.0" layoutY="14.0" minHeight="-Infinity" minWidth="-Infinity" opacity="0.9" prefHeight="201.0" prefWidth="162.0" text="Score">
         <content>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TableView fx:id="tableScore" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="175.0" prefWidth="160.0">
                    <columns>
                      <TableColumn fx:id="columnPlayer" editable="false" prefWidth="102.0" resizable="false" sortable="false" text="Players" />
                      <TableColumn fx:id="columnScore" editable="false" prefWidth="56.0" resizable="false" sortable="false" text="Score" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </Pane>
         </content>
      </TitledPane>
   </children>
</AnchorPane>
